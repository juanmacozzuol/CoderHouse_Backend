<div>
  <h1>Bienvenido {{user.name}} </h1>
    
    <p>Mail:{{user.email}}</p>
    <p>Edad:{{user.age}}</p>
    <p>Rol:{{user.rol}}</p>

</div>
<button id="logout">Logout</button>
<div>
  <table class="table">
    <thead>
      <tr>
        <th scope="col"> Title </th>
        <th scope="col"> Description </th>
        <th scope="col"> Code </th>
        <th scope="col"> Price </th>
        <th scope="col"> Category </th>

      </tr>
    </thead>
    <tbody>
      {{#each productos.docs}}
        <tr>
          <td> {{this.title}}</td>
          <td> {{this.description}}</td>
          <td> {{this.code}}</td>
          <td> {{this.price}} </td>
          <td> {{this.category}}</td>
          <td><button onclick=addItem({{@index}})>Agregar a carrito</button></td>
        </tr>
      {{/each}}

    </tbody>

  </table>
      {{#if productos.prevPage}}
  <button> <a href="/?page={{productos.prevPage}}">Anterior</a></button>
  {{/if}}
    {{#if productos.nextPage}}
  <button> <a href="/?page={{productos.nextPage}}">Siguiente</a></button>
  {{/if}}
  </div>
  <button id="finish">Finalizar compra</button>
</div>  
  <script>    var cart ="{{user.cart}}"
    
  </script>
  <script src="main.js">


  </script>
